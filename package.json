{
  "name": "luco-backend",
  "version": "1.0.0",
  "description": "Scalable multi-tenant SaaS backend for bulk email sending via Amazon SES",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma db push --force-reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "docker:build": "docker build -t luco-backend .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "backup": "chmod +x scripts/backup.sh && ./scripts/backup.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
